{
  "name": "@saeris/eslint-config",
  "version": "2.2.0",
  "description": "Personal collection of shareable ESLint Configs",
  "author": "Drake Costa <drake@saeris.io> (https://github.com/Saeris/)",
  "license": "MIT",
  "keywords": [
    "eslint",
    "config",
    "eslint-config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saeris/eslint-config.git"
  },
  "homepage": "https://github.com/saeris/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/saeris/eslint-config/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=14.x"
  },
  "packageManager": "yarn@3.2.1",
  "main": "src/index.js",
  "files": [
    "src/*",
    "!src/__tests__/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.js",
    "./base": "./src/base.js",
    "./jest": "./src/jest.js",
    "./patch": "./src/patch.js",
    "./next": "./src/next.js",
    "./react": "./src/react.js",
    "./type-aware": "./src/type-aware.js",
    "./typescript": "./src/typescript.js"
  },
  "sideEffects": false,
  "scripts": {
    "ci": "yarn install --ignore-scripts --frozen-lockfile --silent --non-interactive",
    "build:docs": "fiddly",
    "lint": "yarn test:lint",
    "format": "yarn test:lint --fix",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "test": "yarn test:lint && yarn test:typecheck && yarn test:unit",
    "test:lint": "eslint \"./src/**/*.{j,t}s?(x)\"",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "jest",
    "test:watch": "yarn test:unit --watch",
    "typecheck": "yarn test:typecheck",
    "version": "yarn build:docs"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^12.2.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "eslint": ">= 8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/babel__core": "^7.1.19",
    "@types/eslint": "^8.4.5",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "commitlint": "^17.0.3",
    "eslint": "^8.21.0",
    "fiddly": "^0.12.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "release-it": "^15.3.0"
  }
}
